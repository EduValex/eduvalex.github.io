{
  "name": "eduvalex-portfolio",
  "version": "1.0.0",
  "description": "CV Eduardo Valenzuela - Multi-Framework Portfolio",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cd frameworks/react && npm run dev",
    "dev:react": "cd frameworks/react && npm run dev",
    "dev:astro": "cd frameworks/astro && npm run dev",
    "dev:all": "concurrently -n react,astro -c blue,magenta \"npm run dev:react\" \"npm run dev:astro\"",
    "build": "cd frameworks/react && npm run build",
    "build:react": "cd frameworks/react && npm run build",
    "build:astro": "cd frameworks/astro && npm run build",
    "build:all": "npm run build:react && npm run build:astro",
    "preview": "cd frameworks/react && npm run preview",
    "deploy": "node deploy.js",
    "assemble": "node scripts/assemble.js",
  "preview:all": "npm run assemble && node scripts/preview.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eduvalex/eduvalex.github.io.git"
  },
  "keywords": [
    "cv",
    "portfolio",
    "react",
    "multi-framework"
  ],
  "author": "Eduardo Valenzuela",
  "license": "MIT",
  "devDependencies": {
    "gh-pages": "^6.1.1",
    "concurrently": "^8.2.2",
    "serve": "^14.2.4"
  }
}
